        "r = int(input(\"Enter any number:\"))\n",
        "n = 3\n",
        "for i in range(n):\n",
        "  for j in range(n):\n",
        "    if(i + j == n - 1):\n",
        "      print(r, end = \" \")\n",
        "    else:\n",
        "      print(\" \", end =\" \")\n",
        "  print()"
